    "register:spikeprimebtc": "node ./scripts/register.js -C --id=\"spikeprimebtc\" --block=\"./scratch-vm/src/extensions/scratch3_spikeprime_btc\" --entry=\"./scratch-gui/src/lib/libraries/extensions/spikeprimebtc\" --gui=\"../scratch-gui\"",
