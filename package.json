    "register:legoboost": "node ./scripts/register.js -C --id=\"legoboost\" --block=\"./scratch-vm/src/extensions/scratch3_legoboost\" --entry=\"./scratch-gui/src/lib/libraries/extensions/legoboost\" --gui=\"../scratch-gui\"",
